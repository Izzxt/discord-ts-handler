generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type RoomsChannel {
  id String
}

type RoomsUser {
  id String
}

model categories {
  id          String @id @default(auto()) @map("_id") @db.ObjectId
  description String
  key         String
  name        String
  role        String
}

model role_settings {
  id      String @id @default(auto()) @map("_id") @db.ObjectId
  key     String
  role_id String

  @@map("role-settings")
}

model room_settings {
  id  String @id @default(auto()) @map("_id") @db.ObjectId
  id_ String @map("id")
  key String

  @@map("room-settings")
}

model rooms {
  id      String       @id @default(auto()) @map("_id") @db.ObjectId
  channel RoomsChannel
  user    RoomsUser
}
